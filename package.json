    "body-parser": "^1.20.0",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.18"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.45",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
