{
  "name": "api",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "node api/dist/index.js",
    "clean": "rm -rf api/dist",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config ../client/webpack/prod.config.js",
    "build:server": "npm -s run clean && npm -s run generate && tsc",
    "preserve": "npm run build",
    "serve": "npm start",
    "generate": "npm -s run generate:prisma",
    "generate:prisma": "prisma generate --schema=./api/prisma/schema.prisma"
  },
